<%= stylesheet_link_tag "test_taker" %>
<%= javascript_pack_tag "test_taker" %>

<%= tag.div class: 'grey-bar' do %>
  <%= tag.div class: 'green-bar',
              id: 'passing-bar',
              data: { percent: @test_taker.percentage_of_passing } do %>
  <% end %>
  <%= tag.div class: 'red-bar',
              id: 'progress-bar',
              data: { percent: @test_taker.percentage_of_progress } do %>
  <% end %>
<% end %>
